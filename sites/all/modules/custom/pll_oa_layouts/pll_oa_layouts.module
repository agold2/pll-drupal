<?php
/**
 * @file
 * Code for the Open Atrium Layouts feature.
 */

include_once 'pll_oa_layouts.features.inc';

/**
 * Implements hook_ctools_plugin_directory().
 */
function pll_oa_layouts_ctools_plugin_directory($module, $plugin) {
  if ($module == 'ctools') {
    return 'plugins/' . $plugin;
  }
}
